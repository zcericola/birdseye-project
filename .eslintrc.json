{
    "env": {
        "browser": true,
        "es6": true,
        "node": true,
        "jest": true
    },
    "extends": [
        "prettier",
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "plugins": [
        "jest"
    ],
    "rules": {
        "prettier/prettier": [
            "warn"
        ],
        "no-unused-vars": 1,
        "react/prop-types": 0,
        "linebreak-style": [
            "error",
            "unix"
        ],
        "quotes": [
            "error",
            "single"
        ],
        "indent": [
            "warn",
            4
        ],
        "semi": [
            "error",
            "always"
        ]
    },
    "parserOptions": {
        "ecmaVersion": 2017,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "no-case-declarations": "off",
    "no-unsafe-finally": "off",
    "no-unreachable": "warn",
    "no-undef": "warn",
    "no-empty-pattern": "warn"
}